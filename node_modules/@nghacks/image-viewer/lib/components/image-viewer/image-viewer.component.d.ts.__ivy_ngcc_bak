import { OnDestroy, OnInit } from '@angular/core';
import { MatDialogRef } from '@angular/material/dialog';
export declare class ImageDimension {
    width: number;
    height: number;
    constructor(width?: number, height?: number);
}
export interface ImageViewerDialogData {
    src: string;
    alt: string;
}
export declare class ImageViewerComponent implements OnInit, OnDestroy {
    private _nativeDocument;
    private _data;
    private _platformId;
    private _dialogRef;
    src: string;
    alt: string;
    calculatedImageDimension: ImageDimension;
    imageLoaded: boolean;
    hideCloseBtn: boolean;
    private _originalImageDimension;
    private _unsubscribeAll;
    private _document;
    constructor(_nativeDocument: any, _data: ImageViewerDialogData, _platformId: any, _dialogRef: MatDialogRef<ImageViewerComponent>);
    ngOnInit(): Promise<void>;
    private setImageDimension;
    private calculateAspectRatioFit;
    private getOriginalImageDimension;
    private percentage;
    ngOnDestroy(): void;
}
